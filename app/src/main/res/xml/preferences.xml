<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <ListPreference
        android:key="output_preference"
        android:summary="%s"
        android:defaultValue="0"
        android:title="Output"
        android:entries="@array/Output"
        android:entryValues="@array/OutputData"/>

    <EditTextPreference
        android:key="host_preference"
        android:summary=""
        android:title="Server Address"
        android:defaultValue="127.0.0.1"/>

</PreferenceScreen>
